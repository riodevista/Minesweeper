<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:paddingLeft="@dimen/activity_horizontal_margin"
              android:paddingRight="@dimen/activity_horizontal_margin">
    <TextView
        android:id="@+id/uselessId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"/> <!-- Because I am too lazy to make custom cursor adapter right now-->
    <TextView
        android:id="@+id/playerNameInList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/top10_list_text_size"
        android:layout_gravity="start"/>
    <TextView
        android:id="@+id/scoreInList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/top10_list_text_size"
        android:layout_gravity="end"/>
</FrameLayout>